#include <iostream>
#include <cstring>
using namespace std;

int main()
{
    int n, maior = 0, menor = 0;
    do{
    int test = 1;
    cin >> n;
    int p[n];
    if(n != 0){
    string c[n];
    int m[n][12];
    int rank[n];
    //Criando numeração do Ranking
    for(int i = 0; i < n; i++){
        rank[i] = i+1;
    }
    
    for(int i = 0; i < n; i++){
        cin >> c[i];
        for(int j = 0; j < 12; j++){
            cin >> m[i][j];
        }
    }
    for(int i = 0; i < n; i++){
        int soma = 0;
        for(int j = 0; j < 12; j++){
            soma += m[i][j];
            if(j == 0){
                maior = m[i][j];
                menor = m[i][j];
            }
            else{
                if(maior < m[i][j]){
                    maior = m[i][j];
                }
                if(menor > m[i][j]){
                    menor = m[i][j];
                }
            }
        }
        p[i] = soma - maior - menor;
    }
    
    for(int i = 0; i < n-1; i++){
        for(int j = 0; j < n-1; j++){
            if(p[i] < p[i+1]){
                swap(p[i], p[i+1]);
                swap(c[i], c[i+1]);
            }
        }
    }
    for(int i = 0; i < n-1; i++){
        if(p[i] == p[i+1]){
            if(c[i] > c[i+1]){
                swap(c[i], c[i+1]);
                rank[i+1]--;
            }
            else{
                rank[i+1]--;
            }
        }
    }

    cout << "Teste " << test << endl;
    for(int i = 0; i < n; i++){
        cout << rank[i] << " " << p[i] << " " << c[i] << endl; 
    }
    test++;
    }
    cout << endl;
    }while(n > 0);
    
    return 0;
}
